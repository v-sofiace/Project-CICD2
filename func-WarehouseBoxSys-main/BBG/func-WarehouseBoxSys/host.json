{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:05",
      "batchSize": 1,
      "newBatchThreshold": 0,
      "maxDequeueCount": 1
    },
    "http": {}
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },
  "functions": [ "ReceiveShippoNotifications", "UpdateShippoTrackingStatus" ]

}
